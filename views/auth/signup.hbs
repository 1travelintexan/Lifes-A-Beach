<section class="settings homeTop">
  <h3>New here?</h3>
  <h4>Sign up now!</h4>

  <form action="/signup" method="post">
    <label for="username">Username:</label>
    <input type="text" name="username" placeholder="Enter username" >
    <label for="password">Password:</label>
    <input type="password" name="password" placeholder="Enter password" >
    <button type="submit">Submit</button>
  </form>

  <i class="fa fa-arrow-left googleIcon1 or" aria-hidden="true"><a href="/" class="googleIcon1 backT">Back</a></i>
<h4 class=>Or</h4>
  {{#if msg}}
  
  <p>{{msg}}</p> 

  {{/if}}

<i class="fa fa-google fa-3x googleIcon" aria-hidden="false"><a href="/auth/google" class="googleIcon1">oogle</a></i>
</section>


