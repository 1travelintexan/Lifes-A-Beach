<div id="back">
  <a href="/">Go back</a>
</div>

<div class="settings">
  <h1 class="welcome">Welcome, {{ user.username }}!</h1>
  <h2>Create a trip</h2>  
  <a href="/create">Create</a>
 </div>

<h2>Here is a list of your current trips:</h2>
{{#each data}}
